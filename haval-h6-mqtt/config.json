{
  "name": "GWM Brasil com MQTT",
  "version": "0.0.20",
  "slug": "haval-home-assistant",
  "description": "Integração com o Home Assistant usando MQTT para veículos GWM.",
  "init": false,
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "url": "https://github.com/mathtmz/haval-home-assistant",
  "map": ["config:rw"],
  "options": {
    "refresh_time": 30,
    "mqtt_server": "mqtt://homeassistant.local:1883",
    "device_tracker_enabled": true
  },
  "schema": {
    "gwm_username": "email",
    "gwm_password": "password",
    "gwm_vin": "str",
    "gwm_pin": "password?",
    "mqtt_server": "str?",
    "mqtt_user": "str?",
    "mqtt_pass": "password?",
    "refresh_time": "int",
    "device_tracker_enabled": "bool"
  }
}
